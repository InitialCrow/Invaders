<!DOCTYPE html>
<html>
<head>
	<meta charset="utf-8">
	<meta http-equiv="X-UA-Compatible" content="IE=edge">
	<title>tavern Invaders 0.0.2</title>
	<link rel="stylesheet" href="/node_modules/bootstrap/dist/css/bootstrap.min.css">
	<link rel="stylesheet" href="/public/js/lib/Swiper-3.3.1/dist/css/swiper.min.css">
	<link rel="stylesheet" href="/public/css/global.css">
	<link rel="stylesheet" href="/public/css/dashboard.css">
</head>
<body>
	<div class="dashboard wrapper">
		
		<% include ../partials/partial-dashboard-nav %>
		<div class ="profile">
			<section class="main-content col-xs-9 ">
				<div class="search-area">
					<form action="#">

						<h3>Add New friend !</h3>
						<input type="text" name="friend-search" class="form-control">
						<button class="btn btn-default">search</button>
					</form>
					
					
				</div>
				<div class="friend-area">
					<h3>Friend list</h3>
					<hr>
					<ul>
						<%profiles.forEach(function(profile){%>
							<li><a href="friend-profile/<%=profile.user_id%>"><img src="<%= profile.avatar%>" alt="picture of friend's profile" height="50px" width="50px"></a></li>
						<%});%>
						
					</ul>
					
					
				</div>
			</section>
		</div>

		
			
		
	</div>
	<!-- libs -->
	<script src="/public/js/lib/jquery-2.2.3.min.js"></script>
	<script src="/node_modules/bootstrap/dist/js/bootstrap.min.js"></script>

	<!-- scripts -->
	<script src="/public/js/dashboard.js"></script>

	<script>
		app.init();
	</script>

</body>
</html>